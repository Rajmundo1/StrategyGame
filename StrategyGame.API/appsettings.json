{
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "MSSqlServer",
        "Args": {
          "connectionString": "Server=(localdb)\\MSSQLLocalDB; Database=strategyGame; Integrated Security=True;",
          "tableName": "Logs",
          "autoCreateSqlTable": false
        }
      }
    ]
  },
  "ConnectionStrings": {
    "Default": "Server=(localdb)\\MSSQLLocalDB; Database=strategyGame; Integrated Security=True;"
  },
  "Authentication": {
    "JwtNormalIssuer": "strategyGame - normal",
    "JwtLoginIssuer": "strategyGame - login",
    "JwtKey": "SOME_RANDOM_KEY_DO_NOT_SHARE",
    "JwtExpirationMinutes": "120"
  },
  "Frontend": {
    "Url": "http://localhost:4200",
    "SecureUrl": "https://localhost:443"
  },
  "SpaConfiguration": {
    "ApiBaseUrl": "https://localhost:5000",
    "IncomingMessageWebBaseUrl": "https://localhost:5001"
  },
  "AllowedHosts": "*"
}
